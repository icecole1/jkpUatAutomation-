<?xml version="1.0"?>
<!-- For more information on using app.config transformation visit http://go.microsoft.com/fwlink/?LinkId=125889 -->
<!-- In case configuration is not the root element, replace it with root element in source configuration file -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add xdt:Locator="Match(key)" xdt:Transform="SetAttributes(value)" key="Market" value="AU" />
    <add xdt:Locator="Match(key)" xdt:Transform="SetAttributes(value)" key="Environment" value="http://stageredesign.bodenclothing.com.au" />
    <add xdt:Locator="Match(key)" xdt:Transform="SetAttributes(value)" key="UsePreprod" value="false" />
    <add xdt:Locator="Match(key)" xdt:Transform="SetAttributes(value)" key="UseGlobalE" value="false" />
  </appSettings>
</configuration>